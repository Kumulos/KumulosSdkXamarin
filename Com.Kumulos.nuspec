<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
    <metadata>
        <id>Com.Kumulos</id>
        <version>2.0</version>
        <title>Kumulos SDK for Xamarin</title>
        <authors>Kumulos Ltd</authors>
        <owners>Kumulos Ltd</owners>
        <licenseUrl>https://github.com/Kumulos/KumulosSdkXamarin/blob/master/LICENSE</licenseUrl>
        <projectUrl>https://github.com/Kumulos/KumulosSdkXamarin</projectUrl>
        <iconUrl>https://github.com/Kumulos/KumulosSdkXamarin/blob/master/nuget_icon.png?raw=true</iconUrl>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <description>Xamarin SDK For Kumulos</description>
        <summary>Kumulos is a free push notification service for mobile apps. This plugin makes it easy to integrate your Xamarin app with Kumulos</summary>
        <tags>xamarin, monodroid, C#, Kumulos, push, analytics, crash reporting</tags>
        <dependencies>
            <group targetFramework="MonoAndroid9.0">
                <dependency id="Xamarin.Android.Support.v4" version="[28.0.0, 29.0.0)" />
                <dependency id="Xamarin.Android.Support.CustomTabs" version="[28.0.0, 29.0.0)" />
                <dependency id="Xamarin.GooglePlayServices.Gcm" version="[42.1021.1, 71.0.0)" />
            </group>
            <group targetFramework="MonoAndroid8.1">
                <dependency id="Xamarin.Android.Support.v4" version="[27.0.0, 27.2.0)" />
                <dependency id="Xamarin.Android.Support.CustomTabs" version="[27.0.0, 27.2.0)" />
                <dependency id="Xamarin.GooglePlayServices.Gcm" version="[42.1021.1, 60.1143.0)" />
            </group>
            <group targetFramework="MonoAndroid8.0">
                <dependency id="Xamarin.Android.Support.v4" version="[26.0.0, 27.2.0)" />
                <dependency id="Xamarin.Android.Support.CustomTabs" version="[26.0.0, 27.2.0)" />
                <dependency id="Xamarin.GooglePlayServices.Gcm" version="[42.1021.1, 60.1143.0)" />
            </group>
            <group targetFramework="MonoAndroid7.1">
                <dependency id="Xamarin.Android.Support.v4" version="[25.0.0, 26.0.0)" />
                <dependency id="Xamarin.Android.Support.CustomTabs" version="[25.0.0, 26.0.0)" />
                <dependency id="Xamarin.GooglePlayServices.Gcm" version="[42.1021.1, 60.1143.0)" />
            </group>
            <group targetFramework="netstandard1.0">
                <dependency id="NETStandard.Library" version="1.6.1" />
            </group>
        </dependencies>
    </metadata>
    <files>
        <!--Core-->
        <file src="Com.Kumulos\bin\Release\netstandard1.0\Com.Kumulos.pdb" target="lib\netstandard1.0" />
        <file src="Com.Kumulos\bin\Release\netstandard1.0\Com.Kumulos.dll" target="lib\netstandard1.0" />
        <file src="Com.Kumulos.Abstractions\bin\Release\netstandard1.0\Com.Kumulos.Abstractions.pdb" target="lib\netstandard1.0" />
        <file src="Com.Kumulos.Abstractions\bin\Release\netstandard1.0\Com.Kumulos.Abstractions.dll" target="lib\netstandard1.0" />

        <!--Xamarin.Android-->
        <file src="Com.Kumulos.Android\bin\Release\*Kumulos.*" target="lib\MonoAndroid10" />

        <!--Xamarin.iOS Unified-->
        <file src="Com.Kumulos.iOS\bin\Release\*Kumulos.*" target="lib\Xamarin.iOS10" />
    </files>
</package>
